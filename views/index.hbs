<div class="container">
    <div class="row"> {{#each myData}}
        <div class="col-4">


            <div class="card">
                <img src="{{this.imageURL}} " class="card-img-top" alt="...">
                <div class="card-body">
                    <h5 class="card-title">{{this.name}}</h5>
                    <p class="card-text">{{this.type}} </p>
                
                </div>
            </div>
        </div>
        {{/each}}
    </div>

</div>